dotnet publish -c Release -r win10-x64 # compile to win executable
dotnet publish -c Release -r ubuntu.16.10-x64 # ubuntu version
cd bin\Release\netcoreapp2.1\win10-x64\publish\; dir *.exe # see Win executable
cd bin\Release\netcoreapp2.1\ubuntu.16.10-x64\publish\; dir clown* # see ubuntu version
cd bin\debug\netcoreapp2.1;dotnet .\clowncar.dll # run debug
